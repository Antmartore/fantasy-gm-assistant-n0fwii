{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "importOrder": [
    "^@api/(.*)$",
    "^@components/(.*)$", 
    "^@config/(.*)$",
    "^@hooks/(.*)$",
    "^@navigation/(.*)$",
    "^@screens/(.*)$",
    "^@store/(.*)$",
    "^@types/(.*)$",
    "^@utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}