<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CLIENT_ID</key>
    <string>$(FIREBASE_CLIENT_ID)</string>
    <key>REVERSED_CLIENT_ID</key>
    <string>$(FIREBASE_REVERSED_CLIENT_ID)</string>
    <key>API_KEY</key>
    <string>$(FIREBASE_API_KEY)</string>
    <key>GCM_SENDER_ID</key>
    <string>$(FIREBASE_GCM_SENDER_ID)</string>
    <key>PLIST_VERSION</key>
    <string>1</string>
    <key>BUNDLE_ID</key>
    <string>com.fantasygm.assistant.$(ENVIRONMENT_SUFFIX)</string>
    <key>PROJECT_ID</key>
    <string>fantasy-gm-assistant-$(ENVIRONMENT)</string>
    <key>STORAGE_BUCKET</key>
    <string>fantasy-gm-assistant-$(ENVIRONMENT).appspot.com</string>
    <key>IS_ADS_ENABLED</key>
    <false/>
    <key>IS_ANALYTICS_ENABLED</key>
    <string>$(FIREBASE_ANALYTICS_ENABLED)</string>
    <key>IS_APPINVITE_ENABLED</key>
    <true/>
    <key>IS_GCM_ENABLED</key>
    <true/>
    <key>IS_SIGNIN_ENABLED</key>
    <true/>
    <key>GOOGLE_APP_ID</key>
    <string>$(FIREBASE_APP_ID)</string>
    <key>DATABASE_URL</key>
    <string>https://fantasy-gm-assistant-$(ENVIRONMENT).firebaseio.com</string>
    <key>GOOGLE_SIGN_IN_SERVER_CLIENT_ID</key>
    <string>$(FIREBASE_SERVER_CLIENT_ID)</string>
    <key>APPLE_SIGN_IN_SERVICE_ID</key>
    <string>$(APPLE_SIGN_IN_SERVICE_ID)</string>
    <key>FirebaseAppDelegateProxyEnabled</key>
    <true/>
    <key>FirebaseAutomaticScreenReportingEnabled</key>
    <string>$(FIREBASE_ANALYTICS_ENABLED)</string>
    <key>FirebaseDebugEnabled</key>
    <string>$(FIREBASE_DEBUG_MODE)</string>
    <key>FirebaseFirestoreSettings</key>
    <dict>
        <key>persistenceEnabled</key>
        <true/>
        <key>cacheSizeBytes</key>
        <integer>104857600</integer>
        <key>synchronizationMode</key>
        <string>$(FIRESTORE_SYNC_MODE)</string>
    </dict>
    <key>FirebaseStorageSettings</key>
    <dict>
        <key>maxUploadRetryTime</key>
        <integer>120000</integer>
        <key>maxDownloadRetryTime</key>
        <integer>600000</integer>
        <key>maxOperationRetryTime</key>
        <integer>120000</integer>
    </dict>
    <key>FirebaseMessagingSettings</key>
    <dict>
        <key>autoPushNotificationRegistration</key>
        <true/>
        <key>deliveryMetricsExportToBigQuery</key>
        <true/>
    </dict>
    <key>FirebaseAnalyticsConfiguration</key>
    <dict>
        <key>sessionTimeout</key>
        <integer>1800000</integer>
        <key>minimumSessionDuration</key>
        <integer>10000</integer>
        <key>analyticsCollectionEnabled</key>
        <string>$(FIREBASE_ANALYTICS_ENABLED)</string>
    </dict>
</dict>
</plist>